{
    "compilerOptions": {
        "target": "es5",
        "module": "commonjs",
        "strict": true,
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "skipLibCheck": true,
        "types": ["cypress", "cypress-fail-fast", "cypress-file-upload"],
        "resolveJsonModule": true,
        "noEmit": true,
    },
    "include": ["src/**/*.ts", "src/**/*.tsx"],
}
